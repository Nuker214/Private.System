  }
  .box.flashing {
    animation: flashRed 0.4s ease-in-out 2;
  }
  @keyframes flashRed {
    0%, 100% { border-color: red; box-shadow: 0 0 15px red; }
    50% { border-color: white; box-shadow: 0 0 5px white; }
  }
  h2 {
    font-size: 1.7em;
    margin-bottom: 20px;
